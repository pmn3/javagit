package helloworld0;

public class arrays {
	public static void main (String[] args){
		int[] nums = new int[5];
		nums[0] = 1;
		nums[1] = 2;
		nums[2] = 3;
		nums[3] = 4;
		nums[4] = 5;
		System.out.println("***** arrays *****");
		System.out.printf("nums[0]=%d\n",nums[0]);
		System.out.printf("nums[1]=%d\n",nums[1]);
		System.out.printf("nums[2]=%d\n",nums[2]);
		System.out.printf("nums[3]=%d\n",nums[3]);
		System.out.printf("nums[4]=%d\n",nums[4]);
		
		
	}
}
